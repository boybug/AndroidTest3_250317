    package="com.example.yadisak.androidtest3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Boonsiri POS."
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PageActivity.ActCustomer" />
        <activity android:name=".PageActivity.CMD.ActCustomerCmd" />

        <activity android:name=".PageActivity.ActProduct" />
        <activity android:name=".PageActivity.CMD.ActProductCmd" />

        <activity android:name=".PageActivity.ActOrder" />
        <activity android:name=".PageActivity.CMD.ActOrderCmd" />
        <activity android:name=".PageActivity.CMD.ActOrderItemCmd" />

        <activity android:name=".PageActivity.ActBranch" android:noHistory="true"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".ActLogin" android:noHistory="true"/>

    </application>

</manifest>